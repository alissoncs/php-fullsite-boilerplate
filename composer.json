{
  "require": {
      "twig/twig": "^2.0",
      "monolog/monolog": "^1.24",
      "adhocore/jwt": "^0.0.6",
    "slim/twig-view": "^2.5",
    "slim/slim": "^3.0",
    "oscarotero/psr7-middlewares": "^3.21",
    "symfony/dotenv": "^4.3",
    "illuminate/database": "^5.8",
    "spatie/phpunit-watcher": "^1.11",
    "imagine/imagine": "^1.2",
    "guzzlehttp/guzzle": "^6.3",
    "fzaninotto/faker": "^1.8",
    "robmorgan/phinx": "^0.11.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2"
  },
  "repositories": {
    "packagist": { "url": "https://packagist.org", "type": "composer" }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --verbose",
    "migrate": "php vendor/bin/phinx migrate"
  }
}
